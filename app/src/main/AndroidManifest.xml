<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mydictionary">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="activities.DoQuest2Activity"></activity>
        <activity android:name="activities.QuestActivity" />
        <activity android:name="activities.AlarmActivity" />
        <activity android:name="activities.HashtagActivity" />
        <activity android:name="activities.WordMarkActivity" />
        <activity android:name="activities.WordDetailActivity" />
        <activity android:name="activities.AddWordActivity" />
        <activity android:name="activities.ChangeQuestActivity" />
        <activity android:name="activities.DoQuestActivity" />
        <activity android:name="activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver
            android:name=".AlarmReceiver"
            android:enabled="true"
            android:exported="true" />
    </application>

</manifest>