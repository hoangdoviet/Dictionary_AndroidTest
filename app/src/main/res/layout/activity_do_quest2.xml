<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="activities.DoQuest2Activity">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/activity_do_quest2_toolbar"
        app:title="Do Quest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/color_primary_1"/>

    <androidx.recyclerview.widget.RecyclerView
        android:layout_below="@+id/activity_do_quest2_toolbar"
        android:id="@+id/activity_do_quest2_listquest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margin_2"/>

</RelativeLayout>
